{
  "dashboard": {
    "title": "Quantum Computing Dashboard",
    "tags": ["quantum", "qiskit"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Quantum Job Success Rate",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(quantum_jobs_succeeded_total[1h]) / rate(quantum_jobs_total[1h]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": null, "color": "red"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Circuit Execution Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "quantum_circuit_execution_seconds",
            "legendFormat": "{{circuit_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 3,
        "title": "Qubit Fidelity",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "quantum_qubit_fidelity",
            "legendFormat": "Qubit {{qubit_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0.9,
            "max": 1.0
          }
        }
      },
      {
        "id": 4,
        "title": "Job Queue Size",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 8, "y": 0},
        "targets": [
          {
            "expr": "quantum_job_queue_size",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Gate Error Rates",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "quantum_gate_error_rate",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      }
    ]
  }
}
