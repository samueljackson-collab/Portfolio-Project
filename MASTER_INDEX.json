        "duplicate_groups": {
          "fields": ["group_id", "normalized_name", "projects[]"],
          "projects": ["path", "full_name", "has_readme", "file_count"]
        },
        "validation_results": {
          "fields": ["project_name", "status", "issues[]", "warnings[]"],
          "status": ["PASS", "FAIL"]
        }
      },

      "algorithms": {
        "name_normalization": "Remove prefixes (PRJ-, p##, [0-9]*-) → lowercase → replace hyphens/underscores with spaces",
        "quality_scoring": "Sum of documentation(40) + code(30) + structure(20) + recency(10) - placeholders(max 20)",
        "duplicate_detection": "Normalize all project names → group by normalized name → report groups with 2+ projects",
        "version_selection": "Calculate scores for all versions → rank by score → recommend highest score"
      },

      "decision_logic": {
        "keep_vs_merge": "If score difference < 10 points AND other versions have unique content → merge, else keep best",
        "placeholder_handling": "< 5 placeholders → acceptable, > 10 → must fix before validation passes",
        "link_fixing": "Use sed to bulk replace old paths with new paths",
        "validation_retries": "Fix issues → re-validate → repeat until 95%+ pass rate"
      }
    },

