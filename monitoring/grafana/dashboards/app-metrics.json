{
  "title": "Portfolio Metrics",
  "panels": [
    {
      "type": "graph",
      "title": "Request Rate",
      "targets": [{ "expr": "rate(request_count[5m])" }]
    },
    {
      "type": "graph",
      "title": "Request Latency",
      "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(request_duration_seconds_bucket[5m])) by (le))" }]
    },
    {
      "type": "stat",
      "title": "Active Users",
      "targets": [{ "expr": "active_users" }]
    }
  ]
}
